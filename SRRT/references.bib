}

@incollection{Zinkle2012,
title = {1.03 - Radiation-Induced Effects on Microstructure**Prepared for the Oak Ridge National Laboratory under Contract No. DE-AC05-000R22725},
editor = {Rudy J.M. Konings},
booktitle = {Comprehensive Nuclear Materials},
publisher = {Elsevier},
address = {Oxford},
