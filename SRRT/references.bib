	author = {Fausty, Julien and Bozzolo, Nathalie and Bernacki, Marc},
	month = feb,
	year = {2020},
	keywords = {Finite element, Grain growth, Heterogeneous grain boundary energies, Level set, Polycrystal},
	pages = {505--518},
}

