}

@article{ardell_radiation-induced_2016,
	title = {Radiation-induced solute segregation in metallic alloys},
	volume = {20},
	issn = {1359-0286},
	url = {http://www.sciencedirect.com/science/article/pii/S1359028615300097},
	doi = {10.1016/j.cossms.2015.11.001},
	abstract = {The subject of radiation-induced solute segregation (RIS) in metallic alloys is reviewed. RIS manifests itself in several different ways, including diffusion to point-defect sinks (dislocations, grain boundaries, voids, etc.), which can induce precipitation in undersaturated alloys, as well as self-organization of solute clusters and precipitation in defect-free material. Diffusion in dilute and concentrated alloys is highlighted, as are theories of RIS that include new ideas on diffusion of complexes involving coupling between fluxes of point defects and of solute atoms. Many important experimental observations are presented, including up-to-date findings using atom-probe tomography, with special emphasis on solute segregation in austenitic and ferritic steels. Results from computational modeling and theory are also presented and discussed in light of experimental findings. Examples illustrating the factors affecting RIS are shown and some important outstanding issues that impact the current understanding of RIS are described and discussed.},
	language = {en},
	number = {3},
	urldate = {2020-08-27},
	journal = {Current Opinion in Solid State and Materials Science},
	author = {Ardell, Alan J. and Bellon, Pascal},
	month = jun,
	year = {2016},
	keywords = {Austenitic and ferritic stainless steels, Clustering, Computational modeling, Diffusion, Radiation damage, Rate-theory kinetics, Solute segregation},
	pages = {115--139},
}

